{
  "name": "magic-eye",
  "description": "A magic eye app",
  "version": "0.9.3",
  "type": "module",
  "scripts": {
    "check-format": "prettier . --check",
    "format": "prettier . --write --log-level silent",
    "lint": "eslint . --ext .tsx",
    "dev": "eslint . --ext .tsx  &&  vite --host",
    "build": "tsc && vite build",
