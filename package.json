    "@tauri-apps/api": "^2.0.0-rc.5",
    "@tauri-apps/plugin-app": "^2.0.0-alpha",
    "@tauri-apps/plugin-dialog": "^2.2.0",
    "@tauri-apps/plugin-http": "^2.0.1",
    "@tauri-apps/plugin-window": "^2.0.0-alpha",
    "framer-motion": "^11.15.0",
    "hls-video-element": "^1.2.5",
    "hls.js": "^1.5.17",
    "media-chrome": "^4.2.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
